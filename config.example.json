{
  "connections": [
    {
      "label": "production",
      "host": "db.example.com",
      "port": 5432,
      "user": "readonly_user",
      "password": "your_password",
      "database": "myapp",
      "enabled": true,
      "ssl": true,
      "readOnly": true,
      "poolSize": 5
    },
    {
      "label": "cloud-rds",
      "url": "postgresql://user:pass@rds-instance.amazonaws.com:5432/mydb?sslmode=require",
      "enabled": true,
      "ssl": true,
      "readOnly": true
    },
    {
      "label": "staging",
      "host": "staging-db.example.com",
      "port": 5432,
      "user": "dev",
      "password": "dev_password",
      "database": "myapp_staging",
      "enabled": true,
      "readOnly": false
    },
    {
      "label": "local",
      "host": "localhost",
      "port": 5432,
      "user": "postgres",
      "password": "postgres",
      "database": "devdb",
      "enabled": false
    }
  ]
}
